{
	"name": "Selenized Dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#fbf3db",
				"foreground": "#53676d" // fg_0
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#909995" // dim_0
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#00978a" // cyan
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {

				"foreground": "#a27e00" // bright yellow
			}
		},
		{
			"name": "Constants (including numbers)",
			"scope": "constant",
			"settings": {
				// "foreground": "#3a4d53", // pink
			}
		},
		{
			"name": "Language constants (booleans)",
			"scope": "constant.language",
			"settings": {
				"foreground": "#c44392"
			}
		},
		{
			"name": "Formatting placeholders, escaped characters",
			"scope": [
				"constant.other.placeholder",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#006dce", // blue
			}
		},
		{
			"name": "constant.other",
			"scope": "constant.other",
			"settings": {
				// "foreground": "#825dc0" // violet
			}
		},
		{
			"name": "Keyword control flow",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#bc5819" // orange
			}
		},
		{
			"name": "Storage types",
			"scope": [
				"storage.type",
				"keyword.type",
			],
			"settings": {
				"foreground": "#53676d" // fg_0
			}
		},
		{
			"name": "Storage modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				// "foreground": "#825dc0"
			}
		},
		{
			"name": "entity.name",
			"scope": "entity.name",
			"settings": {
				"foreground": "#a78300" // violet
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#006dce" // blue
			}
		},

		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#53676d"
			}
		},
		{
			"name": "Function parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				// "foreground": "#3a4d53",
			}
		},
		// {
		// 	"name": "Variable start",
		// 	"scope": "punctuation.definition.variable",
		// 	"settings": {
		// 		"foreground": "#428b00"
		// 	}
		// },

		{
			"name": "Tag attribute (HTML)",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#006dce" // blue
			}
		},
		{
			"name": "Library/builtin function",
			"scope": "support.function",
			"settings": {
				"foreground": "#006dce" // blue
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#428b00" // green
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#cc1729" // red
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#c50924" // bright red
			}
		}
	],
